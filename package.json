{
  "name": "blake3-jit",
  "version": "1.0.0",
  "description": "The pure JavaScript BLAKE3 implementation with runtime WASM SIMD",
  "keywords": [
    "blake3",
    "crypto",
    "fast",
    "hash",
    "simd",
    "wasm"
  ],
  "license": "MIT",
  "author": "LongYinan <github@lyn.one>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brooooooklyn/blake3-jit.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
