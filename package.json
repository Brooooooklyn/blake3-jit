    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "oxlint-tsgolint": "^0.10.1",
    "rolldown": "1.0.0-beta.58",
    "rolldown-plugin-dts": "^0.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
